{{ define "block" }}

<div>
  <ul>
    <li><b>Height:</b> {{ .Height }}</li>

    {{ if .PreviousHash }}
    <li><b>Previous hash:</b> {{ .PreviousHash }}</li>
    {{
      end
    }}

    <li><b>Hash:</b> {{ .Hash }}</li>

    <li><b>Difficulty:</b> {{ .Difficulty }}</li>

    <li><b>NOnce:</b> {{ .NOnce }}</li>

    <li><b>Timestamp:</b> {{ .Timestamp }}</li>

    <li><b>Transactions:</b> {{ .Transactions }}</li>
  </ul>
</div>
<hr />

{{ end }}
